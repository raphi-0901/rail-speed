{
  "name": "rail-speed",
  "version": "1.0.0",
  "description": "GNOME Shell extension showing current train speed.",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "link": "ln -sf $(pwd)/dist ~/.local/share/gnome-shell/extensions/rail-speed@raphael-wirnsberger.at",
    "reset": "gnome-extensions reset rail-speed@raphael-wirnsberger.at",
    "enable": "gnome-extensions enable rail-speed@raphael-wirnsberger.at",
    "disable": "gnome-extensions disable rail-speed@raphael-wirnsberger.at",
    "reload": "gnome-extensions disable rail-speed@raphael-wirnsberger.at && gnome-extensions enable rail-speed@raphael-wirnsberger.at"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raphi-0901/rail-speed.git"
  },
  "keywords": [
    "gnome-shell",
    "extension",
    "train",
    "typescript",
    "backoff"
  ],
  "author": "Raphael Wirnsberger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raphi-0901/rail-speed/issues"
  },
  "homepage": "https://github.com/raphi-0901/rail-speed#readme",
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@girs/adw-1": "^1.9.0-4.0.0-beta.38",
    "@girs/clutter-17": "^17.0.0-4.0.0-beta.38",
    "@girs/gdk-4.0": "^4.0.0-4.0.0-beta.38",
    "@girs/gio-2.0": "^2.86.0-4.0.0-beta.38",
    "@girs/glib-2.0": "^2.86.0-4.0.0-beta.38",
    "@girs/gnome-shell": "^49.1.0",
    "@girs/gobject-2.0": "^2.86.0-4.0.0-beta.38",
    "@girs/gtk-4.0": "^4.20.1-4.0.0-beta.38",
    "@girs/meta-17": "^17.0.0-4.0.0-beta.38",
    "@girs/mtk-17": "^17.0.0-4.0.0-beta.38",
    "@girs/pango-1.0": "^1.57.0-4.0.0-beta.38",
    "@girs/shell-17": "^17.0.0-4.0.0-beta.38",
    "@girs/st-17": "^17.0.0-4.0.0-beta.38",
    "ava": "^6.4.1",
    "typescript": "^5.9.3"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "out/src/",
        "test/": "out/test/"
      },
      "compile": false
    }
  }
}
